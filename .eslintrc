{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "plugins":[
  ],
  "env": {
    "jquery": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
  },
  "globals": {
    "jQuery": true,
    "$": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-var": 2,
    "semi": [2, "never"],
    "no-mixed-spaces-and-tabs": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 2
      }
    ],
    "no-extra-boolean-cast": 0,
    "comma-dangle": 1,
    "no-console": 1
  }
}